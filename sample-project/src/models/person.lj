# Person model

export class Person {
  private const name: Str
  private mut age: Int
  
  constructor(name: Str, age: Int) {
    this.name = name
    this.age = age
  }
  
  public fn getName(): Str {
    return this.name
  }
  
  public fn getAge(): Int {
    return this.age
  }
  
  public fn setAge(age: Int) {
    this.age = age
  }
  
  public fn getInfo(): Str {
    return "${this.name} is ${this.age} years old"
  }
  
  public fn birthday() {
    this.age = this.age + 1
  }
}
